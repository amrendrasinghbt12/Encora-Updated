<?xml version="1.0" ?>
<!--
/**
 * Customproduct Script Integration
 *
 * @category  Customproduct
 * @package   Customproduct_Script
 */
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<tab id="customproduct" sortOrder="999" translate="label">
			<label>customproduct</label>
		</tab>
		<section id="product" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label">
			<label>product</label>
			<tab>customproduct</tab>
			<resource>Customproduct_Script::config_customproduct_script</resource>
			<group id="product" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label">
				<label>product</label>
				<field id="module" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="select">
					<label>module</label>
					<comment/>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
				</field>
			</group>
		</section>
	</system>
</config>
